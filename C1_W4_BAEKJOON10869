void main( )
{
    int a;
    int b;
    int p;
    int m;
    int g;
    int nu;
    int nm;
    
    scanf("%d%d",&a,&b);
    
    p=a+b;
    m=a-b;
    g=a*b;
    nu=a/b;
    nm=a%b;
    
    printf("%d\n",p);
    printf("%d\n",m);
    printf("%d\n",g);
    printf("%d\n",nu);
    printf("%d\n",nm);
    
    //BAEKJOON10869 (https://www.acmicpc.net/problem/10869)
}
